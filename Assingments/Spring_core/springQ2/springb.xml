<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
        
      <beans> 
  
 <bean id="obj" class="spring2.question">  
<constructor-arg value="1"></constructor-arg>  
<constructor-arg value="what is java"></constructor-arg> 



<constructor-arg type="List">

<list>

<value>java is popular programming language</value>
<value>java is platform indepedent</value>
<value>java is used of application development</value>
</list>

</constructor-arg>
<constructor-arg type="Set">
<set>
<value>java is popular programming language</value>
<value>java is platform indepedent</value>
<value>java is used of application development</value>
</set>
</constructor-arg>
<constructor-arg type="Map">
<map>
 <entry key="1" value="java is popular programming language"/>           
            <entry key="2" value="java is platform indepedent"/>

</map>
</constructor-arg>


</bean>  
</beans>  